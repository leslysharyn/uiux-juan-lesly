<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="MediaAlarm" >
        <item name="android:forceDarkAllowed">false</item>
        <item name="android:textAllCaps">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/buttonDark</item>
        <item name="colorPrimaryDark">@color/background_variant</item>
        <item name="colorPrimaryVariant">@color/background_variant</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="lollipop">@color/background_variant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>